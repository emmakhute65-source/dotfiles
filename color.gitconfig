[color]
    ui = true
[color "grep"]
    match = 33
[color "status"]
    header = 8
    added = 10
    changed = 9
    untracked = 3
    branch = 0 5

    # Because you want an even more unnerving detatched head state
    nobranch = bold ul blink 1

[color "diff"]
    meta = 8
    func = 6
    frag = 8
    old = 9
    context = 7
    new = 10

