Config { font = "-*-*-medium-*-*-*-14-*-*-*-*-*-*-*"
       , bgColor = "#1d1f21"
       , fgColor = "#c5c8c6"
       , position = TopW L 100
       , commands = [
                    Run Com "weathermajig" ["80501"] "weathermajig" 300,
                    -- Run Weather "KDEN" [
                    --                     "--template"
                    --                     ,"<fc=#969896>DIA:</fc><tempF>Â°F"
                    --                     ] 36000
                    --,
                    Run Cpu [
                              "--template", "<total>%"
